{
  "name": "jeremy.codes",
  "version": "1.0.0",
  "description": "My personal website.",
  "main": "index.js",
  "scripts": {
    "clean": "npx rimraf dist",
    "dev": "npm run clean && NODE_ENV=development node server.js",
    "build": "npm run clean && NODE_ENV=production npx webpack --progress",
    "analyze": "npx webpack --profile --progress --json > bundle-stats.json && npx webpack-bundle-analyzer bundle-stats.json",
    "audit": "npm run build && npx bundle-buddy dist/js/*.map",
    "start": "NODE_ENV=production npm run build && NODE_ENV=production node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/malchata/jeremy.codes.git"
  },
  "author": "Jeremy L. Wagner <jeremy.l.wagner@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/malchata/jeremy.codes/issues"
  },
  "homepage": "https://github.com/malchata/jeremy.codes#readme",
  "dependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.4",
    "bundle-buddy": "^0.2.1",
    "copy-webpack-plugin": "^4.5.3",
    "core-js": "^2.5.7",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.4",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "imagemin-webpack-plugin": "^2.3.0",
    "mini-css-extract-plugin": "^0.4.4",
    "on-demand-live-region": "^0.1.3",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "preact": "^8.3.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "uglify-js": "^3.4.9",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "worker-loader": "^2.0.0"
  },
  "browserslist": [
    "last 3 versions"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false,
          "useBuiltIns": "usage"
        }
      ]
    ],
    "plugins": [
      [
        "@babel/transform-react-jsx",
        {
          "pragma": "h"
        }
      ],
      "@babel/syntax-dynamic-import"
    ]
  }
}
