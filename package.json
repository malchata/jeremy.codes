{
  "name": "jeremy.codes",
  "version": "1.1.0",
  "description": "My personal website.",
  "main": "./src/index.js",
  "config": {
    "lastBuild": ""
  },
  "scripts": {
    "clean": "npx rimraf dist",
    "dev": "npm run clean && NODE_ENV=development node server.js",
    "build": "npm run clean && npm run webpack:prod && npm run webp:png",
    "webpack:prod": "NODE_ENV=production npx webpack --progress && npm run inject-scripts",
    "inject-scripts": "node ./scripts/inject-scripts.js",
    "webp:png": "find ./dist -type f -name '*.png' | xargs -P 8 -I {} sh -c 'npx cwebp -lossless -q 100 -z 9 -m 6 $1 -o \"${1%.png}.webp\"' _ {} \\;",
    "analyze": "npm run clean && NODE_ENV=production npx webpack --profile --progress --json > bundle-stats.json && npx webpack-bundle-analyzer bundle-stats.json",
    "audit": "npm run build && npx bundle-buddy dist/js/*.map",
    "lint": "npx eslint --no-inline-config ./src/**/*.js",
    "start": "NODE_ENV=production npm run build && NODE_ENV=production node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/malchata/jeremy.codes.git"
  },
  "author": "Jeremy L. Wagner <jeremy.l.wagner@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/malchata/jeremy.codes/issues"
  },
  "homepage": "https://github.com/malchata/jeremy.codes#readme",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "on-demand-live-region": "^0.1.3",
    "preact": "^8.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "assets-webpack-plugin": "^3.9.7",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "brotli-webpack-plugin": "^1.0.0",
    "bundle-buddy": "^0.2.1",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "cssnano": "^4.1.7",
    "cwebp-bin": "^4.0.0",
    "eslint": "^5.9.0",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.4",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "imagemin-webp": "^4.1.0",
    "imagemin-webpack-plugin": "^2.3.0",
    "mini-css-extract-plugin": "^0.4.5",
    "null-loader": "^0.1.1",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "preact-render-to-string": "^4.1.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.1.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.26.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2"
  }
}
